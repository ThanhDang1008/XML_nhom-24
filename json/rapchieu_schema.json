{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "RapChieu",
  "type": "object",
  "properties": {
    "rapchieu": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "MaRap": { "type": "integer", "minimum": 1 },
          "TenRap": { "type": "string" },
          "DiaChi": { "type": "string" },
          "SucChua": { "type": "integer", "minimum": 1 },
          "SoDienThoai": { "type": "string", "pattern": "^[0-9]{10,11}$" }
        },
        "required": ["MaRap", "TenRap", "DiaChi"]
      }
    }
  },
  "required": ["rapchieu"]
}
